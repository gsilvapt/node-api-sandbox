{
  "name": "todo-api",
  "version": "1.0.0",
  "description": "An API Sandbox",
  "main": "server/server.js",
  "scripts": {
    "start": "node server/server.js",
    "test": "export NODE_ENV=test || SET \"NODE_ENV=test\" && mocha server/**/*.test.js",
    "test-watch": "nodemon --exec 'npm test'",
    "coverage": "nyc --reporter=text mocha server/**/*.test.js",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls"
  },
  "engines": {
    "node": "9.5.0"
  },
  "author": "gsilvapt",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "lodash": "^4.17.5",
    "mongodb": "^2.2.5",
    "mongoose": "^4.5.9"
  },
  "devDependencies": {
    "coveralls": "^3.0.0",
    "expect": "^1.20.2",
    "mocha": "^3.0.2",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.10.2",
    "nyc": "^11.4.1",
    "supertest": "^2.0.0"
  }
}
